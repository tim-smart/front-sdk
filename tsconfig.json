{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es2017",
    "lib": ["esnext"],
    "noUnusedParameters": true,
    "noUnusedLocals": true
  },
  "include": ["typings/**/*.d.ts", "lib/*.ts"]
}
