{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "jsdoc-format": false,
    "whitespace": [false, "check-type"],
    "only-arrow-functions": false,
    "no-var-requires": false,
    "no-console": false,
    "trailing-comma": false,
    "no-string-literal": false,
    "interface-name": false,
    "no-namespace": false
  }
}
